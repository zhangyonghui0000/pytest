names = ["aa","a","zyh"]
print(names[-1],names[1])
print(names[0:3])
print(names[-2]) #倒着取
print(names[-3:])
names.append("zyh--")
print(names)
names.insert(1,"zyh--001")
print(names)
names[2]="zyh-----"
names.remove("zyh--001")
del names[1]
names.pop()#删掉最后一个
print(names)
names.pop()
print(names)
names.append("zyh")
print(names)
names.pop(0)
print(names)

print(names[names.index("zyh")])
print(names.count("zyh"))
names.reverse()
print(names)
names.sort()
print(names)

